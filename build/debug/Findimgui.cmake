
message(STATUS "Conan: Using autogenerated Findimgui.cmake")
# Global approach
SET(imgui_FOUND 1)
SET(imgui_INCLUDE_DIRS "/home/scott/.conan/data/imgui/1.61/bincrafters/stable/package/1f6e0209a27dccc2729e2a3f36e0964d7aa48cfb/include")
SET(imgui_INCLUDES "/home/scott/.conan/data/imgui/1.61/bincrafters/stable/package/1f6e0209a27dccc2729e2a3f36e0964d7aa48cfb/include")
SET(imgui_DEFINITIONS )
SET(imgui_LIBRARIES "") # Will be filled later
SET(imgui_LIBRARIES_TARGETS "") # Will be filled later, if CMake 3
SET(imgui_LIBS "") # Same as imgui_LIBRARIES

mark_as_advanced(imgui_FOUND imgui_INCLUDE_DIRS imgui_INCLUDES
                 imgui_DEFINITIONS imgui_LIBRARIES imgui_LIBS)


# Find the real .lib/.a and add them to imgui_LIBS and imgui_LIBRARY_LIST
SET(imgui_LIBRARY_LIST imgui)
SET(imgui_LIB_DIRS "/home/scott/.conan/data/imgui/1.61/bincrafters/stable/package/1f6e0209a27dccc2729e2a3f36e0964d7aa48cfb/lib")
foreach(_LIBRARY_NAME ${imgui_LIBRARY_LIST})
    unset(CONAN_FOUND_LIBRARY CACHE)
    find_library(CONAN_FOUND_LIBRARY NAME ${_LIBRARY_NAME} PATHS ${imgui_LIB_DIRS}
                 NO_DEFAULT_PATH NO_CMAKE_FIND_ROOT_PATH)
    if(CONAN_FOUND_LIBRARY)
        if(${CMAKE_VERSION} VERSION_LESS "3.0")
            list(APPEND imgui_LIBRARIES ${CONAN_FOUND_LIBRARY})
        else() # Create a micro-target for each lib/a found
            set(_LIB_NAME CONAN_LIB::imgui_${_LIBRARY_NAME})
            add_library(${_LIB_NAME} UNKNOWN IMPORTED)
            set_target_properties(${_LIB_NAME} PROPERTIES IMPORTED_LOCATION ${CONAN_FOUND_LIBRARY})
            list(APPEND imgui_LIBRARIES_TARGETS ${_LIB_NAME})
        endif()
        message(STATUS "Found: ${CONAN_FOUND_LIBRARY}")
    else()
        message(STATUS "Library ${_LIBRARY_NAME} not found in package, might be system one")
        list(APPEND imgui_LIBRARIES_TARGETS ${_LIBRARY_NAME})
        list(APPEND imgui_LIBRARIES ${_LIBRARY_NAME})
    endif()
endforeach()
set(imgui_LIBS ${imgui_LIBRARIES})

if(NOT ${CMAKE_VERSION} VERSION_LESS "3.0")
    # Target approach
    if(NOT TARGET imgui::imgui)
        add_library(imgui::imgui INTERFACE IMPORTED)
        if(imgui_INCLUDE_DIRS)
          set_target_properties(imgui::imgui PROPERTIES
          INTERFACE_INCLUDE_DIRECTORIES "${imgui_INCLUDE_DIRS}")
        endif()
        set_property(TARGET imgui::imgui PROPERTY INTERFACE_LINK_LIBRARIES ${imgui_LIBRARIES_TARGETS} "" "")
        set_property(TARGET imgui::imgui PROPERTY INTERFACE_COMPILE_DEFINITIONS )
        set_property(TARGET imgui::imgui PROPERTY INTERFACE_COMPILE_OPTIONS "" "")
    endif()
    
endif()
